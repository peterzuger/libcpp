#ifndef __STREAMOFF__
#define __STREAMOFF__

#if !defined(__STREAMOFF_DEFINED__)
# define __STREAMOFF_DEFINED__
namespace std{
    using streamoff = long long;
}
#endif /* !defined(__STREAMOFF_DEFINED__) */

#endif /* __STREAMOFF__ */
