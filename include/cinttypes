/**
 * @file   cinttypes
 * @author Peter Zueger
 * @date   27.07.2018
 * @brief  C Format conversion of integer types
 */
#ifndef __CINTTYPES__
#define __CINTTYPES__

#include <inttypes.h>

namespace std{
    // types
    using ::imaxdiv_t;
    using ::wchar_t;

    using ::imaxabs;
    using ::imaxdiv;

    using ::strtoimax;
    using ::strtoumax;

    using ::wcstoimax;
    using ::wcstoumax;
}

#endif /* __CINTTYPES__ */
