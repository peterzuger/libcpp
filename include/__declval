#ifndef ____DECLVAL__
#define ____DECLVAL__

#include <type_traits>

namespace std{
    // 23.2.7, declval
    template<class T>
    std::add_rvalue_reference_t<T> declval()noexcept; // as unevaluated operand
}

#endif /* ____DECLVAL__ */
